<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-reaper-project">
        <comment>Reaper Project</comment>
        <icon name="fm.reaper.Reaper.x-reaper-project"/>
        <glob pattern="*.RPP"/>
    </mime-type>
    <mime-type type="application/x-reaper-project-backup">
        <comment>Reaper Project Backup</comment>
        <icon name="fm.reaper.Reaper.x-reaper-backup"/>
        <glob pattern="*.RPP-bak"/>
    </mime-type>
    <mime-type type="application/x-reaper-theme">
        <comment>Reaper Theme</comment>
        <icon name="fm.reaper.Reaper.x-reaper-theme"/>
        <glob pattern="*.ReaperThemeZip"/>
    </mime-type>
</mime-info>
